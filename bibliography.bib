	Pages = {137-177},
	Title = {{A}pplication of prototype flume tests for beach nourishment assessment},
	Url = {http://www.sciencedirect.com/science/article/B6VCX-472BJFN-1/2/48676d2e6db8371d74002216fc660e5e},
