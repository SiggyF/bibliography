	Keywords = {beach, dune, erosion, lidar, shoreline},
	Number = {F3},
	Pages = {n/a--n/a},
	Title = {Probabilistic prediction of barrier-island response to hurricanes},
	Url = {http://dx.doi.org/10.1029/2011JF002326},
