	Journal = {Coastal Engineering},
	Number = {3},
	Pages = {221 - 245},
	Title = {Numerical simulation of time-dependent beach and dune erosion},
	Url = {http://www.sciencedirect.com/science/article/pii/0378383985900092},
	Volume = {9},
