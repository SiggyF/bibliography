	Volume = {66},
	Year = {2006},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0272771405002714},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.ecss.2005.07.021}}

