	Note = {The CoastView Project: Developing coastal video monitoring systems in support of coastal zone management},
	Number = {6-7},
	Pages = {477-491},
	Title = {{T}he history and technical capabilities of {A}rgus},
	Url = {http://www.sciencedirect.com/science/article/B6VCX-4NH6CRS-1/2/3887cadd383f1d2a1cc1c596ac49cbf3},
