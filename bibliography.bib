	Keywords = {sea level projection},
	Number = {3},
	Pages = {647--667},
	Publisher = {Blackwell Publishing Ltd},
	Title = {On Postglacial Sea Level},
	Url = {http://dx.doi.org/10.1111/j.1365-246X.1976.tb01252.x},
